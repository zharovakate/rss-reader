@{
    ViewBag.Title = "Authors";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="row">
    <div class="col-md-12">
        @foreach (var blog in ViewBag.Authors)
        {
            <p>
                @blog.Key
                @blog.Value
            </p>
        }
    </div>
</div>