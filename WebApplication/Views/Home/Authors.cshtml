@{
    ViewBag.Title = "Authors";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="row">
    <div class="col-md-12">
        @foreach (var author in ViewBag.Authors)
        {
            <div class="col-md-7">
                @author.Key
            </div>
            
            <div class="col-md-5">
                <p>
                    @author.Value
                </p>
            </div>
        }
    </div>
</div>